class Add 
{
	public static void main(String[] args) 
	{
		System.out.println(add (20,30));
		Scanner scn=new Scanner(System.in);
		System.out.println("enter 2 nums");
		int x=scn.nextInt();
		int y=scn.nextInt();
		System.out.println(add(x,y));
	}
	static int add (int a,int b) 
	{
		return a+b;
	}
}
