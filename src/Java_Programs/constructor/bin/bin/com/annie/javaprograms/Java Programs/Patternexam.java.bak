import java.util.Scanner;
class Pattern#*
{
	public static void main(String[] args) 
	{ Scanner scn=new Scanner(System.in);
	System.out.print("enter a size");
	  int n = scn.nextInt();
	  for(int i = 1;i<=n;i++)
		{ int a =n+1;
		  for(int j =1 ; j<=n;j++)
			{ a--;
			  if (i!=j)
			  System.out.print("*");
			  else
		System.out.print("#");
	}System.out.println();
}
	}
}